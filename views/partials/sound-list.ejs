<style>
    .sound-item{
        border-left-style: solid;
        border-width: 2px;
        border-color: #155598;
        margin: 7.5px 5px;
        padding: 5px;
    }
</style>
<div id="sound-list">
    <% for (var i = 0; i < soundList.length; i++) { %>
        <div class="sound-item">
            <a style="padding:5px;display: flex;justify-content: space-between;"> 
                <div><%= soundList[i]["name"] %></div>
                <div><%= soundList[i]["count"] %></div>
            </a>
        </div>
     <% } %>  
</div>
